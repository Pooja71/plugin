{@eq key=.meta value="struct"}
    types["{.id}"] = reader.structure(
        {.preferredListEncoding},
        {@math key=.dataWordCount method="multiply" operand=8/},
        {@math key=.pointerCount method="multiply" operand=8/}
    );

    {#.nodes}
        {>"rTypes/structure"/}
    {/.nodes}
{/eq}
