{@eq key=.meta value="enum"}
    types['{.id}'] = {
        {#.enumerants}
            {.name} : {$idx}
            {@sep},{/sep}
        {/.enumerants}
    };
{/eq}

{?recurTarget}
    {@eq key=.meta value=recurTarget}
        {#.nodes}
            {>"rTypes/enumeration" recurTarget=recurTarget/}
        {/.nodes}
    {/eq}
{/recurTarget}
