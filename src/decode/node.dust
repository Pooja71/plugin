{@select key=.meta}
    {@eq value="struct"}
        {>structure/}
    {/eq}

    {@eq value="enum"}
        {>enumeration/}
        {?.nodes}
            {@ctThrow message="Invariant violated: enum nodes cannot have any child nodes."/}
        {/.nodes}
    {/eq}

    {@eq value="const"}
        {>constant/}
    {/eq}
{/select}
